@page
@model FinalProject.Front.Pages.Exams.DeleteModel

@{
    ViewData["Title"] = "Delete";
}

<h1>Delete Certificate</h1>

<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>Certificate</h4>
    <div>
        <h4>@Model.Certificate.Title </h4>
    </div>
    
    <form method="post">
        <input type="hidden" asp-for="Certificate.Id" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a class="btn btn-primary" asp-page="./Index">Cancel</a>
    </form>
</div>




