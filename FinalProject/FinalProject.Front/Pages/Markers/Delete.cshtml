@page
@model FinalProject.Front.Pages.Markers.DeleteModel
@{
ViewData["Title"] = "Delete";
}

<h1>Delete Marker</h1>

<h3>Are you sure you want to delete this user?</h3>
<div>
    <h4>@Model.Marker.FirstName @Model.Marker.LastName</h4>
    <p>Email: @Model.Marker.Email</p>
</div>

<form method="post">
    <input type="hidden" asp-for="Marker.Id" />
    <input type="submit" value="Delete" class="btn btn-danger" /> |
    <a class="btn btn-primary" asp-page="./Index">Cancel</a>
</form>
